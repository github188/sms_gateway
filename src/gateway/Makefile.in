SUBDIRS = common channeldll gateway gateway_send manager uniqio

recursive:
	 for i in $(SUBDIRS);\
	 do \
		 cd $$i;\
		 gmake ;\
		 if [ $$? -ne 0 ] ; then\
			exit 1;\
			fi; \
			cd ..;\
		 done;\

install:
	for i in $(SUBDIRS);\
	do \
		 cd $$i;\
		 gmake install;\
		 if [ $$? -ne 0 ] ; then\
			 exit 1;\
		 fi; \
		 cd ..;\
  	done;\


clean:
	for i in $(SUBDIRS);\
	do \
		 cd $$i;\
		 gmake clean;\
		 if [ $$? -ne 0 ] ; then\
			 exit 1;\
		 fi; \
		 cd ..;\
       done;\


