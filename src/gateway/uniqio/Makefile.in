SVC_OBJ= $(CMM_OBJ) uniqio.o uniqio_task.o uniqio_biz.o uniqio_db.o uniqio_util.o 

SVC_SRC=$(SVC_OBJ:.o=.cpp)
 
EXT_CPPFLAGS = -I@prefix@/base \
			   -I@prefix@/src/gateway/3rd/include \
               -I@prefix@/src/gateway/channeldll \
			   -I@prefix@/src/gateway/common 
 
