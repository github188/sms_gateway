	cp -f $(SVC_BIN) @prefix@/bin/$(SVC_BIN).1.$$SUBVERSION; \
	cd @prefix@/bin; \
	ln -fs $(SVC_BIN).1.$$SUBVERSION $(SVC_BIN)
 
